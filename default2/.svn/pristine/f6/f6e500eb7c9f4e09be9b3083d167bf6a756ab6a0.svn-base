/* 图文详情 */

.details {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
   margin: 0;
  padding: 0;
}
.details  image{
  margin: 0;
  padding: 0;
  display:flex;
