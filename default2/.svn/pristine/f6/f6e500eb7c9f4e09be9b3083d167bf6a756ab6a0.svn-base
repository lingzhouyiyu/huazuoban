@import "../../wxParse/wxParse.wxss";
.container {
  background-color: white;
  padding-bottom: 100rpx;
}

page {
  background-color: white;
}

.flowertitle {
  width: 92%;
  font-size: 32rpx;
  font-weight: bold;
  margin-top: 20rpx;
}

.describe {
  width: 92%;
  font-size: 26rpx;
  margin-top: 20rpx;
  color: #999;
}

.priceoptions {
  width: 92%;
  display: flex;
  margin-top: 20rpx;
  justify-content: flex-start;
  align-items: center;
}

.newprices {
  font-size: 32rpx;
  color: #ff4081;
  font-weight: bold;
  /* margin-right: 7rpx; */
}

.newprice {
  font-size: 32rpx;
  color: #ff4081;
  font-weight: bold;
  margin-right: 15rpx;
}

.oldprice {
  font-size: 20rpx;
  color: #888;
  text-decoration: line-through;
}

.tipoptionswrap {
  width: 100%;
  display: flex;
  height: 60rpx;
  justify-content: center;
  align-items: center;
  margin-top: 30rpx;
  background-color: #fafafa;
}

.tipoptions {
  width: 92%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  height: 74rpx;
}

.optionsitem {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-right: 25rpx;
}

.optionsitem image {
  width: 30rpx;
  height: 30rpx;
}

.optionsitem text {
  font-size: 24rpx;
  color: #999;
  margin-left: 10rpx;
}

.quan {
  width: 92%;
  height: 90rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.quantitle {
  font-size: 28rpx;
  font-weight: bold;
  margin-right: 20rpx;
}

.quanprice {
  height: 48rpx;
  background-color: #ff4081;
  padding: 2rpx 15rpx;
  font-size: 24rpx;
  color: #fff;
}

.quanright image {
  width: 25rpx;
  height: 25rpx;
}

.sales {
  width: 92%;
  height: auto;
  padding: 20rpx 0;
  display: flex;
}

.salestitle {
  width: 9%;
  margin-right: 20rpx;
  font-size: 28rpx;
  font-weight: bold;
}

.salesdetail {
  width: 91%;
  display: flex;
  flex-direction: column;
}

.salesitem {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  /* background-color: yellow; */
  margin-bottom: 10rpx;
}

.salesitemleft {
  width: 15%;
  margin-top: -4rpx;
}

.salesbtn {
  font-size: 18rpx;
  border: 1rpx solid #ff4081;
  color: #ff4081;
  padding: 5rpx 20rpx;
}

.salesitemright {
  width: 85%;
  font-size: 22rpx;
}

/* 底部按钮 */

.bottomBtn {
  width: 100%;
  height: 90rpx;
  position: fixed;
  display: flex;
  bottom: 0;
  left: 0;
  color: #fff;
  font-size: 32rpx;
}

.icon {
  width: 12%;
  background-color: #fafafa;
  display: flex;
  justify-content: center;
  align-items: center;
}

.icon image {
  width: 52rpx;
  height: 52rpx;
}
.collect{
   width: 13%;
   height: 100%;
   background-color: #fafafa;
   display: flex;
   flex-direction: column;
   justify-content: center;
   align-items: center;
   font-size: 24rpx;   
   color: #707070;
}
.collectimg{
width:45rpx;
height: 45rpx;
}
.order {
  width: 75%;
  background-color: #ff4081;
  display: flex;
  justify-content: center;
  align-items: center;
}

