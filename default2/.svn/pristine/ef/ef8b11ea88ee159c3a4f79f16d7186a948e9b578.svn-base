page{
  background-color: #f2f2f2;
}
.contentwrap{
  width: 94%;
  height: auto;
  margin-top: 20rpx;
  /* background-color: red; */
  display: flex;
  flex-flow: row wrap;
  justify-content: space-between;
}
.contentitem{
  width: 48.5%;
  height: auto;
  background-color: #fff;
display: flex;
flex-direction: column;
justify-content: flex-start;
align-items: center;
margin-bottom: 20rpx;
}
.contentitem image{
width: 100%;
}
.contentitem text{
width: 100%;
height: 80rpx;
display: flex;
justify-content: center;
align-items: center;
}